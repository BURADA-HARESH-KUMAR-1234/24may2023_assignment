#include<stdio.h>

void main()
{
  int a[100],b[100],c[100];
  int i, n1,n2,j;
  printf("enter the size of array 1");
  scanf("%d",&n1);
  printf("enter %d elements",n1);
  for(i=0;i<n1;i++){
  scanf("%d",&a[i]);}
  printf("enter the size of array 2");
  scanf("%d",&n2);
  printf("enter %d elements ",n2);
  for(i=0;i<n2;i++){
  scanf("%d",&b[i]);}
  for(i=0;i<n1;i++)
  c[i]=a[i];
  for(i=0;i<n2;i++)
  c[i+n1]=b[i];
  for(i=0;i<(n1+n2);i++)
  printf("%d ",c[i]);
}









