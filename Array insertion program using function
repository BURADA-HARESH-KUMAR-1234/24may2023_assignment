#include<stdio.h>
 
 void ins(int a[],int n)
 {
   int i, s;
   printf("\nenter the element to insert");
   scanf("%d",&s);
   for(i=0;i<=n;i++)
   {
     if(i==n){
     a[i]=s;
     }
     printf("%d ",a[i]);
   }
 }
 void main()
 {
   int a[100], n, i, s;
   scanf("%d",&n);
   printf("enter %d elements",n);
   for(i=0;i<n;i++)
   {
     scanf("%d",&a[i]);
   }
   for(i=0;i<n;i++)
   printf("%d ",a[i]);
   ins(a,n);
   
 }






